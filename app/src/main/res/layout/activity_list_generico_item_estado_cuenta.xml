<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="5dip"
    android:layout_margin="2dp"
    android:id="@+id/CanchitaItemLaoyout" >



    <TextView
        android:id="@+id/generic_field_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:visibility="invisible"
        android:typeface="sans"
        android:textSize="14dip"
        android:textStyle="bold"/>



    <TextView
        android:id="@+id/generic_field_1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_id"
        android:layout_marginTop="1dip"
        android:text=""
        android:textColor="#343434"
        android:textSize="16dip"
        />

    <TextView
        android:id="@+id/generic_field_2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_1"
        android:textColor="#343434"
        android:textSize="16dip"
        android:layout_marginTop="1dip"
        android:text=""
        android:textStyle="bold"

        />


    <TextView
        android:id="@+id/generic_field_3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_2"
        android:gravity="left"
        android:text=""
        android:textSize="16dip"
        android:textColor="@color/maroon"
        />

    <TextView
        android:id="@+id/generic_field_4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_3"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:textColor="@color/blue"
        />

    <TextView
        android:id="@+id/generic_field_5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_4"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />

    <TextView
        android:id="@+id/generic_field_6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_5"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />

    <TextView
        android:id="@+id/generic_field_7"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_6"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />

    <TextView
        android:id="@+id/generic_field_8"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_7"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />


    <TextView
        android:id="@+id/generic_field_9"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_8"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />


    <TextView
        android:id="@+id/generic_field_10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/generic_field_9"
        android:gravity="left"
        android:text=""
        android:layout_marginRight="5dip"
        android:textSize="16dip"
        android:visibility="visible"
        />



</RelativeLayout>